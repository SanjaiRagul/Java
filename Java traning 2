// to find the sequnce number based on input
public class Main
{
	public static void main(String[] args) {
		int n= new java.util.Scanner(System.in).nextInt();
		for(int i=1;i<=n;i++){
		    if(i%2==0){
		        int res=(i*i)-2;
		        System.out.print(res+" ");
		    }
		    else{
		        int res=(i*i)-1;
		        System.out.print(res+" ");
		    }
		}
	}
}
******************************************************************************************************************************************************************
// Collatz conjecture sequence
public class Main
{
	public static void main(String[] args) {
		int n= new java.util.Scanner(System.in).nextInt();
		int temp=0;
		while(n>1) {
			if(n%2==0) {
				n=n/2;
				System.out.println(n);
				temp++;
			}
			else{
			    n=3*n+1;
			    System.out.println(n);
			    temp++;
			}
			
		}
		
		System.out.println("total value:"+temp);
		
	}
}
