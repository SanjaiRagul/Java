// to find the second largest number and find the GCD
import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    Scanner sc=new Scanner(System.in);
		int i1=10;
		int i2=2;
		int i3=9;
		int i=1;
		if(i1>i2 && i1<i3){
		    System.out.println(i1);
		}
		else if(i2>i1 && i2<i3){
		    System.out.println(i2);
		}
		else
		    System.out.println(i3);
		 
		while(i>=0){
		    i++;
		    if(i1%i==0 && i2%i==0 && i3%i==0){
		        System.out.println("key is"+i);
		        break;
		    }
		}
		
	}
}
*******************************************************************************************************************************************************************
// to calculate the total crr and trr
import java.util.*;
public class Main
{
	public static void main(String[] args) {
		int total_balls=300;
		int total_runs=375;
		int runs_scored=78;
		int balls_bowled=45;
		
		int n_o_overs =total_balls/6;
		System.out.println("n_o_overs:"+n_o_overs);
		
		float o_finished= (balls_bowled/6)+(balls_bowled%6)*0.1f;
		System.out.println("o_finished:"+o_finished);
		
		float crr = runs_scored/o_finished;
	    System.out.printf("CRR: %.1f%n", crr);
		
		float trr = (float)total_runs/n_o_overs;
		System.out.println("trr: "+trr);
		
		if(crr==trr||crr>trr){
		    System.out.println("Win");
		}
		else
		System.out.println("Loss");
		
	}
}
