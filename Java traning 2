//Mango problem to find the number present in the c and r
import java.util.*;
public class Main
{
	public static void main(String[] args) {
	   int i1=5,i2=i1;
	   int b=25;
	   int r =(b-1)/5;
	   int c =(b-1)%5;
	   if(b>25){
	       System.out.println("Invalid");
	   }
	   else if(c==0 || c==4 || c%b==1){
	        System.out.println("Mango Tree");
	   }
	   else{
	       System.out.println("Not Mango Tree");
	   }
	}
}
// other method
import java.util.*;
public class Main
{
	public static void main(String[] args) {
	   Scanner sc=new Scanner(System.in);  
	   int a=sc.nextInt(); 
	   int b=sc.nextInt(); 
	   int m=sc.nextInt();  
	   if(m>a*b) System.out.println("Invalid"); 
	   else{ 
	       if(m <= a || m%a == 1||m%a == 0) System.out.println("Mango tree"); 
	       else System.out.println("Not Mango");
	   }
	}
}
******************************************************************************************************************************************************************
// leap year
import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    int y = new Scanner(System.in).nextInt();
        boolean leap = (y % 400 == 0) || (y % 4 == 0 && y % 100 != 0);
        System.out.println(leap ? "Leap Year" : "Not Leap Year");
	}
}
*****************************************************************************************************************************************************************
// to find the sequnce number based on input
public class Main
{
	public static void main(String[] args) {
		int n= new java.util.Scanner(System.in).nextInt();
		for(int i=1;i<=n;i++){
		    if(i%2==0){
		        int res=(i*i)-2;
		        System.out.print(res+" ");
		    }
		    else{
		        int res=(i*i)-1;
		        System.out.print(res+" ");
		    }
		}
	}
}
