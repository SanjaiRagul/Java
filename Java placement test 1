//Write a Program to Print the Biggest Number out of the Given three Numbers?

import java.util.*;
class Main
{
    public static void main(String []args)
    {
        Scanner sc=new Scanner(System.in);
        int num1 = sc.nextInt();
        int num2 = sc.nextInt();
        int num3 = sc.nextInt();
        int big = num1;
        if(num2>big){
            big=num2;
        }
        if(num3>big){
            big=num3;
        }
        System.out.println(big+" is a Biggest Number from the Given Numbers");
    }
}
