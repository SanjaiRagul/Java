//write a program to convert kg values into gram values

import java.util.*;
class Main{
    public static void main(String[] args){
        Scanner sc= new Scanner(System.in);
        double kg = sc.nextDouble();
        double gs =(kg)*1000;
        System.out.println((int)gs+" Grams");
    }
}
