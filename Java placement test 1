//Write a Program to identiy the given number is Prime or Not Prime
import java.util.*;
class Main
{
    public static void main (String []args)
    {
        Scanner sc=new Scanner(System.in);
        int num1 = sc.nextInt();
        if(num1%2==0){
            System.out.println("Prime Number");
        }
        else if(num1%2!=0){
            System.out.println("Not a Prime Number");
        }
        else if (num1 <1){
            System.out.println("Not a Valid input");
        }
    }
}

//Write a program to print all Odd Numbers in Given Range.if starting range is greater than ending range print "INVALID RANGE"

import java.util.*;
class Main{
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        int n = sc.nextInt();
        int n2 = sc.nextInt();
        if(n<n2){
            for (int i=n; i<=n2; i++){
                if(i%2!=0){
                    System.out.print(i+" ");
                    
                }
            }
        }
        else{
            System.out.println("INVALID RANGE");
        }
    }
}


//write a program to print all character by using char variable

import java.util.*;
class Main{
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        char ch=sc.next().charAt(0);
        if (ch =='a'){
            for (char ch1='a'; ch1<='z'; ch1++){
                System.out.print(ch1+" ");
            }
        }
        if (ch =='A'){
            for (char ch1='A'; ch1<='Z'; ch1++){
                System.out.print(ch1+" ");
            }
        }
        
    }
}
