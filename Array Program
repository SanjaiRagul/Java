//
import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int arr[]=new int[n];
		int c=1;

		for(int i=0; i<n; i++) {
			arr[i]=sc.nextInt();
		}
		Arrays.sort(arr);
		System.out.println("Sorted array");
		for(int i=0; i<n; i++) {
			System.out.print(arr[i]+" ");
		}
		System.out.println();
		System.out.println("Result");
		System.out.print(arr[0]+" ");
		for(int i=1; i<n; i++) {
			if(arr[i]!=arr[i-1]){
			    System.out.print(arr[i]+" ");
			} c++;
			
		}
	}
}
