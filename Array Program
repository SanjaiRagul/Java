// Basic Program
import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    Scanner sc = new Scanner(System.in);
		int[] roll = new int[5];
		
		for(int i=1; i< roll.length; i++){
		    System.out.printf("Enter the Grade "+i +":");
		    roll[i] = sc.nextInt();
		}
		System.out.println("your Grade");
		for(int i=1; i< roll.length; i++){
		    System.out.println("rollno:"+i + ":" +roll[i]);
		}
		
	}
}
