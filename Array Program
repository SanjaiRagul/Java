// 2d array Program
import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    Scanner sc=new Scanner(System.in);
	    System.out.print("Enter the Size of rows:");
	    int rows = sc.nextInt();
	    System.out.print("Enter the Size of colums:");
	    int colums = sc.nextInt();
	    int mat[][] = new int[rows][colums];
	    int sum=0;
	    System.out.println("Enter the marix elem: ");
	    for(int i=0; i<rows; i++){
	        for(int j=0; j<colums; j++){
	            mat[i][j]=sc.nextInt();
	            
	        }
	    }
	    System.out.println("matrix is: ");
	    for(int i=0; i<rows; i++){
	        for(int j=0; j<colums; j++){
	            System.out.print(mat[i][j]+" ");
	        }
	        System.out.println();
	    }
	    
	    
	}
}
********************************************************************************************************************************************************************
// add the 2 matrix
import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    Scanner sc=new Scanner(System.in);
	    System.out.print("Enter the Size of rows:");
	    int rows = sc.nextInt();
	    System.out.print("Enter the Size of colums:");
	    int colums = sc.nextInt();
	    int a[][] = new int[rows][colums];
	    int b[][] = new int[rows][colums];
	    int sum[][] = new int[rows][colums];
	   
	    System.out.println("Enter the A marix elem: ");
	    for(int i=0; i<rows; i++){
	        for(int j=0; j<colums; j++){
	            a[i][j]=sc.nextInt();
	            
	        }
	    }
	    System.out.println("Enter the B marix elem: ");
	    for(int i=0; i<rows; i++){
	        for(int j=0; j<colums; j++){
	            b[i][j]=sc.nextInt();
	            
	        }
	    }
	    for(int i=0; i<rows; i++){
	        for(int j=0; j<colums; j++){
	            sum[i][j]=a[i][j] + b[i][j];
	            
	        }
	    }
	    System.out.println("matrix is: ");
	    for(int i=0; i<rows; i++){
	        for(int j=0; j<colums; j++){
	            System.out.print(sum[i][j]+" ");
	        }
	        System.out.println();
	    }
	    System.out.println();
	    
	}
}
